	"type": "module",
	"packageManager": "pnpm@7.9.5",
	"devDependencies": {
		"@cloudflare/workers-types": "3.17.0",
		"@types/jest": "29.1.2",
		"@typescript-eslint/eslint-plugin": "5.40.0",
		"@typescript-eslint/parser": "5.40.0",
		"esbuild": "0.15.11",
