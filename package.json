{
	"private": true,
	"name": "workers",
	"version": "0.0.0",
	"repository": "git@github.com:dhcmrlchtdj/workers.git",
	"author": "h11 <nirisix@gmail.com>",
	"license": "Apache-2.0",
	"type": "module",
	"devDependencies": {
		"@cloudflare/workers-types": "4.20240620.0",
		"@jest/globals": "29.7.0",
		"@types/node": "20.14.7",
		"esbuild": "0.21.5",
		"jest": "29.7.0",
		"oxlint": "0.4.4",
		"prettier": "3.3.2",
		"prettier-plugin-organize-imports": "3.2.4",
		"typescript": "5.5.2",
		"wrangler": "3.61.0"
	},
	"pnpm": {
		"overrides": {
			"@types/node": "20.8.3"
		}
	},
	"prettier": {
		"printWidth": 80,
		"tabWidth": 4,
		"useTabs": true,
		"semi": false,
		"singleQuote": false,
		"quoteProps": "as-needed",
		"jsxSingleQuote": false,
		"trailingComma": "all",
		"bracketSpacing": true,
		"bracketSameLine": false,
		"arrowParens": "always",
		"proseWrap": "preserve",
		"htmlWhitespaceSensitivity": "ignore",
		"endOfLine": "lf",
		"embeddedLanguageFormatting": "auto",
		"singleAttributePerLine": true,
		"plugins": [
			"prettier-plugin-organize-imports"
		]
	}
}
