{
	"private": true,
	"name": "workers",
	"version": "0.0.0",
	"repository": "git@github.com:dhcmrlchtdj/workers.git",
	"author": "h11 <nirisix@gmail.com>",
	"license": "Apache-2.0",
	"type": "module",
	"devDependencies": {
		"@cloudflare/workers-types": "4.20250204.0",
		"@jest/globals": "29.7.0",
		"@types/node": "22.13.1",
		"esbuild": "0.25.0",
		"jest": "29.7.0",
		"oxlint": "0.15.10",
		"prettier": "3.5.0",
		"prettier-plugin-organize-imports": "4.1.0",
		"typescript": "5.7.3"
	},
	"prettier": {
		"printWidth": 80,
		"tabWidth": 4,
		"useTabs": true,
		"semi": false,
		"singleQuote": false,
		"quoteProps": "as-needed",
		"jsxSingleQuote": false,
		"trailingComma": "all",
		"bracketSpacing": true,
		"bracketSameLine": false,
		"arrowParens": "always",
		"proseWrap": "preserve",
		"htmlWhitespaceSensitivity": "ignore",
		"endOfLine": "lf",
		"embeddedLanguageFormatting": "auto",
		"singleAttributePerLine": true,
		"plugins": [
			"prettier-plugin-organize-imports"
		]
	}
}
