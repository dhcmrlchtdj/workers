	"license": "Apache-2.0",
	"type": "module",
	"devDependencies": {
		"@cloudflare/workers-types": "4.20250816.0",
		"@jest/globals": "30.0.5",
		"@mitata/counters": "0.0.8",
		"@types/node": "24.3.0",
		"@typescript/native-preview": "7.0.0-dev.20250816.1",
		"esbuild": "0.25.9",
		"jest": "30.0.5",
		"mitata": "1.0.34",
		"oxlint": "1.12.0",
		"oxlint-tsgolint": "0.0.4",
		"prettier": "3.6.2",
		"prettier-plugin-organize-imports": "4.2.0"
	},
	"prettier": {
		"printWidth": 80,
		"tabWidth": 4,
		"useTabs": true,
		"semi": false,
		"singleQuote": false,
		"quoteProps": "as-needed",
		"jsxSingleQuote": false,
		"trailingComma": "all",
		"bracketSpacing": true,
		"bracketSameLine": false,
		"arrowParens": "always",
		"proseWrap": "preserve",
		"htmlWhitespaceSensitivity": "ignore",
		"endOfLine": "lf",
		"embeddedLanguageFormatting": "auto",
		"singleAttributePerLine": true,
		"plugins": [
			"prettier-plugin-organize-imports"
		]
	}
}
